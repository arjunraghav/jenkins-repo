pipeline {
    agent any

    stages {
        stage('stage1') {
            steps {
                echo 'Hello World1'
            }
        }
        stage('stage2') {
            steps {
                echo 'Hello World2'
            }
        }
        stage('stage3') {
            steps {
                echo 'Hello World2 from git'
            }
        }
    }
}
